# Called when the node enters the scene tree for the first time.
func _ready() -> void:
	var player = $Player
	var custom_camara = $CustomCamara
	custom_camara.position = Vector3(player.position.x, 2.5, 6)


# Called every frame. 'delta' is the elapsed time since the previous frame.
func _process(delta: float) -> void:
	var player = $Player
	var custom_camara = $CustomCamara
	
	custom_camara.position = Vector3(player.position.x + player.camara_offset, 2.5, 6)
	
